resource "aws_security_group" "{##SECURITY_GROUP##}" {
  name        = "{##NAME##}"
  description = "Security Group"
  vpc_id      = aws_vpc.main_vpc.id

  {##INGRESS##}

  egress {
    from_port   = 0
    to_port     = 0
    protocol    = "-1"
    cidr_blocks = ["0.0.0.0/0"]
  }

  tags = {
    Name = "{##NAME##}"
  }
}
